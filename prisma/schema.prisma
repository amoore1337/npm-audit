datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Package {
  id String @id @default(cuid())
  name String @unique
  latestVersion String
  versions String // No List support in SQLite
  homepage String?
  repo String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
